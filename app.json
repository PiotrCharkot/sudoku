{
  "expo": {
    "name": "Sudoku",
    "slug": "sudoku",
    "sdkVersion": "43.0.0",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sudokuLogoThick.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "host.exp.sudoku2000",
      "usesAppleSignIn": true
    },
    "android": {
      "package": "host.exp.sudoku2000" ,
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/sudokuLogoThick.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
